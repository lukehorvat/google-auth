{
  "name": "googleapis-auth",
  "version": "1.0.1",
  "description": "Generate a credentials (token) file for authorized access to Google APIs.",
  "author": "Luke Horvat",
  "license": "MIT",
  "repository": "lukehorvat/googleapis-auth",
  "bin": "dist/index.js",
  "scripts": {
    "build": "node_modules/.bin/babel lib -d dist -q",
    "prebuild": "node_modules/.bin/rimraf dist",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "bluebird": "3.4.6",
    "chalk": "1.1.3",
    "google-auth-library": "0.9.10",
    "mkdirp": "0.5.1",
    "nightmare": "2.9.0",
    "yargs": "4.6.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "rimraf": "2.5.4"
  },
  "keywords": [
    "google",
    "api",
    "googleapis",
    "auth",
    "credentials",
    "token",
    "client"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
